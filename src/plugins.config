#
# Plugin configuration file for Image/J 1.31s or newer
#

# Author:   Jarek Sacha
# Version:  @BUILD_VERSION@
# Date:     @BUILD_DATE@
# Requires: ImageJ 1.31s


#-------------------------------------------------------------------------
# 3D IO
#
Plugins>3D IO, "MetaImage Reader ...", net.sf.ij.io.metaimage.MetaImageReaderPlugin
Plugins>3D IO, "MetaImage Writer ...", net.sf.ij.io.metaimage.MetaImageWriterPlugin
Plugins>3D IO, "VTK Reader ...", net.sf.ij.io.vtk.VtkReaderPlugin
Plugins>3D IO, "VTK Writer ...", net.sf.ij.io.vtk.VtkWriterPlugin


#-------------------------------------------------------------------------
# 3D Toolkit
#
Plugins>3D Toolkit, "Connected Threshold Grower ...", net.sf.ij.im3d.grow.ConnectedThresholdGrowerPlugin
Plugins>3D Toolkit, "Auto Clip Volume", net.sf.ij.im3d.filters.AutoClipVolumePlugin
Plugins>3D Toolkit, "Morphological Dilate 3D", net.sf.ij.im3d.filters.MorphologicalDilate3DPlugin
Plugins>3D Toolkit, "Morphological Erode 3D", net.sf.ij.im3d.filters.MorphologicalErode3DPlugin


#-------------------------------------------------------------------------
# Color
#

# Conversion between color spaces
Plugins>Color, "RGB to CIE L*a*b* stack", net.sf.ij_plugins.color.ConvertRGBToLabStackPlugin
Help>About Plugins>Color, "RGB to CIE L*a*b* stack...", net.sf.ij_plugins.color.ConvertRGBToLabStackPlugin("about")
Plugins>Color, "CIE L*a*b* stack to RGB", net.sf.ij_plugins.color.ConvertLabStackToRGBPlugin
Help>About Plugins>Color, "CIE L*a*b* stack to RGB...", net.sf.ij_plugins.color.ConvertLabStackToRGBPlugin("about")

# Edge detection in color and multiband images
Plugins>Color, "Multiband Sobel edges", net.sf.ij_plugins.multiband.VectorEdgeDetectorPlugin("VectorSobelEdgeOperator")
Help>About Plugins>Color, "Multiband Sobel edges...", net.sf.ij_plugins.multiband.VectorEdgeDetectorPlugin("about")
#Plugins>Color, "Multiband Difference edges", net.sf.ij_plugins.multiband.VectorEdgeDetectorPlugin("VectorDifferenceEdgeOperator")
#Plugins>Color, "Multiband Gradient edges", net.sf.ij_plugins.multiband.VectorEdgeDetectorPlugin("VectorGradientEdgeOperator")

# Intensity shift
Plugins>Color, "Intensity shift", net.sf.ij_plugins.color.IntensityShiftPlugin
Help>About Plugins>Color, "Intensity shift...", net.sf.ij_plugins.color.IntensityShiftPlugin("about")


#-------------------------------------------------------------------------
# Segmentation
#
Plugins>Segmentation, "k-means Clustering ...", net.sf.ij.clustering.KMeansClusteringPlugin("")
Help>About Plugins, "k-means Clustering...", net.sf.ij.clustering.KMeansClusteringPlugin("about")

Plugins>Segmentation, "Maximum Entropy Threshold", net.sf.ij.thresholding.MaximumEntropyThresholdPlugin("")
Help>About Plugins, "Maximum Entropy Threshold...", net.sf.ij.thresholding.MaximumEntropyThresholdPlugin("about")

Plugins>Segmentation, "Maximum Entropy Multi-Threshold", net.sf.ij.thresholding.MaximumEntropyMultiThresholdPlugin("")
Help>About Plugins, "Maximum Entropy Multi-Threshold...", net.sf.ij.thresholding.MaximumEntropyMultiThresholdPlugin("about")


#-------------------------------------------------------------------------
# Image Quilter
#
Plugins>Graphics, "Image Quilter ...", net.sf.ij_plugins.quilting.ImageQuilterPlugin
Help>About Plugins, "Image Quilter....", net.sf.ij_plugins.quilting.ImageQuilterPlugin("about")
