#
# Plugin configuration file for Image/J 1.31s or newer
#

# Author:   Jarek Sacha
# Version:  @BUILD_VERSION@
# Date:     @BUILD_DATE@
# Requires: ImageJ 1.31s

Plugins>Color, "Color Histogram", net.sf.ij.plugin.VtkColorHistogram
Plugins>Color, "Intensity shift", net.sf.ij.plugin.IntensityShift
Plugins>Color, "Convert RGB to CIELab stack", net.sf.ij.plugin.ConvertRGBToLabStack
Plugins>Color, "Sobel Vector Edge Detector", net.sf.ij.plugin.VectorEdgeDetector("SobelEdgeOperator")
Plugins>Color, "Difference Vector Edge Detector", net.sf.ij.plugin.VectorEdgeDetector("DifferenceVectorEdgeOperator")
Plugins>Color, "Vector Gradient Edge Detector", net.sf.ij.plugin.VectorEdgeDetector("VectorGradientEdgeOperator")
#Help>About Plugins, "k-means Clustering...", net.sf.ij.plugin.KMeansClustering("about")
